<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="com.github.computeronfire.yahtzee.GameController"
          stylesheets="/UI.css"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Button text="Main Menu" onAction="#exitToMenu"
            GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button text="Test" onAction="#sayHelloWorld"
            GridPane.rowIndex="0" GridPane.columnIndex="1"/>


    <Label GridPane.rowIndex="1" GridPane.columnIndex="0" fx:id="helloWorld"/>
</GridPane>
